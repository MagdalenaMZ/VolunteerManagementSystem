@model IEnumerable<string>
<a class="btn btn btn-outline-dark btn-block m-2"
       asp-action="Index"
       asp-controller="Admin"
       asp-route-category="">
        EDIT/DELETE/ADD
</a>
<a class="btn btn btn-outline-dark btn-block m-2"
   asp-action="List"
   asp-controller="Volunteer"
   asp-route-category="">
    All
</a>
@foreach (string category in Model)
    {
    <a class="btn btn-block m-2
@(category == ViewBag.SelectedCategory ? "btn-primary": "btn-outline-dark")"
       asp-action="List"
       asp-controller="Volunteer"
       asp-route-category="@category"
       asp-route-page="1">
        @category
    </a>
}